run = "python3 main.py"
modules = ["python-3.10:v18-20230807-322e88b"]

[nix]
channel = "stable-22_11"

[env]
PYTHONPATH = "${PYTHONPATH}:${workspaceFolder}"

[languages.python]
pattern = "**/*.py"
syntax = "python"

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"
build = ["pip", "install", "-r", "requirements.txt"]

[[ports]]
localPort = 5000
externalPort = 80
